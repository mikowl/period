			"color": "#944cd7",
			"strikethrough": false,
			"underline": false,
			"backgroundColor": "transparent",
			"bold": false,
			"italic": false
		},
		{
			"tag": "#",
			"color": "#FFBE76",
			"strikethrough": false,
			"underline": false,
			"backgroundColor": "transparent",
			"bold": false
